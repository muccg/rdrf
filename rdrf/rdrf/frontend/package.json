{
  "name": "cicproms",
  "version": "2.0.0",
  "description": "Frontend code for cicproms",
  "private": true,
  "dependencies": {
    "@types/jest": "29.1.1",
    "@types/lodash": "^4.14.186",
    "@types/node": "18.7.23",
    "@types/rc-slider": "^9.3.0",
    "@types/rc-tooltip": "^3.7.7",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/reactstrap": "^8.7.1",
    "@types/redux-actions": "^2.6.2",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.1",
    "child_process": "^1.0.2",
    "lodash": "^4.17.21",
    "npm-watch": "^0.11.0",
    "rc-slider": "^10.0.1",
    "rc-tooltip": "^5.2.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.4",
    "react-scripts": "5.0.1",
    "reactstrap": "^9.1.4",
    "redux": "^4.2.0",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.4.1",
    "typescript": "4.8.4",
    "yarn": "^1.22.19"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "./postbuild.sh",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-rename": "npm run build-rename-js && npm run build-rename-css",
    "build-rename-js": "renamer --find '/main(.*)/' --replace 'main-bundle.min.js' build/static/js/*.js && renamer --find '/runtime(.*)/' --replace 'runtime-bundle.min.js' build/static/js/*.js && renamer --find '/^\\d(.*)/' --replace 'vendors-bundle.min.js' build/static/js/*.js ",
    "build-rename-css": "renamer --find '/main(.*)/' --replace 'main.css' build/static/css/*.css  && renamer --find '/^\\d(.*)/' --replace 'vendors.css' build/static/css/*.css ",
    "lint": "tslint 'src/**/*.ts{,x}'",
    "watch": "npm-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "fs": "^0.0.1-security",
    "moxios": "^0.4.0",
    "prettier": "^2.7.1",
    "renamer": "^4.0.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "watch": "^1.0.2"
  }
}
